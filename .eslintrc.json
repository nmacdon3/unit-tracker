{
  "extends": ["next/core-web-vitals", "eslint:recommended"],
  "rules": {
    "react/no-unescaped-entities": "warn",
    "import/order": [
      "warn",
      {
        "newlines-between": "always",
        "pathGroups": [
          {
            "pattern": "~/**",
            "group": "external",
            "position": "after"
          }
        ]
      }
    ]
  }
}
